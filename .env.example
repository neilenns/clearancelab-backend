# Port the HTTP server listens on
PORT=3000

# Full MongoDB connection string, including credentials and params
MONGO_DB_CONNECTION_STRING=mongodb://user:pass@localhost:27017

# Database name within the cluster
MONGO_DB_NAME=my_app

# Log level: error, warn, info, debug. Default info.
LOG_LEVEL=debug

# Node environment: production, development, test. Default development.
NODE_ENV=development
